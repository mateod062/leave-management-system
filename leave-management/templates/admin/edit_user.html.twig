{# templates/admin/edit_user.html.twig #}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Edit User</title>
    <link rel="stylesheet" href="{{ asset('css/style.css') }}">
</head>
<body>
<div class="edit-user-container">
    <h1>Edit User</h1>
    {{ form_start(form) }}
    {{ form_row(form.username) }}
    {{ form_row(form.email) }}
    {{ form_row(form.role) }}
    {{ form_row(form.team) }}
    {{ form_row(form.submit) }}
    {{ form_end(form) }}
</div>
</body>
</html>
